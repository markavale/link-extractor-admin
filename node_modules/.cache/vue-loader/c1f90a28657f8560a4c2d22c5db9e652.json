odel=\"permanent\"\n        class=\"ma-2\"\n        label=\"Permanent\"\n      ></v-switch>\n      <v-switch\n        v-model=\"miniVariant\"\n        class=\"ma-2\"\n        label=\"Mini variant\"\n      ></v-switch>\n      <v-switch\n        v-model=\"expandOnHover\"\n        class=\"ma-2\"\n        label=\"Expand on hover\"\n      ></v-switch>\n      <v-switch\n        v-model=\"background\"\n        class=\"ma-2\"\n        label=\"Background\"\n      ></v-switch>\n      <v-switch\n        v-model=\"right\"\n        class=\"ma-2\"\n        label=\"Right\"\n      ></v-switch>\n    </v-row> -->\n  <div>\n    <!-- <v-card height=\"100vh\" class=\"overflow-hidden\"> -->\n    <v-app-bar app color=\"grey darken-4\" dark>\n      <v-app-bar-nav-icon\n        @click.stop=\"sidebarMenu = !sidebarMenu\"\n      ></v-app-bar-nav-icon>\n      <v-toolbar-title>MMI ADMIN SCRAPER</v-toolbar-title>\n      <v-spacer></v-spacer>\n\n      <v-menu offset-y dark outline v-if=\"isAuthenticated\">\n        <template v-slot:activator=\"{ on }\">\n          <v-btn dark icon v-on=\"on\">\n            <v-icon>mdi-account</v-icon>\n          </v-btn>\n        </template>\n        <v-card class=\"mx-auto\" elevation=\"11\" max-width=\"300\" tile>\n          <v-list shaped>\n            <v-subheader>Hello, {{ user.username }}</v-subheader>\n            <v-divider></v-divider>\n            <v-list-item-group v-model=\"item\" color=\"primary\">\n              <v-list-item\n                v-for=\"(item, i) in userDetails\"\n                :key=\"i\"\n                :to=\"item.href\"\n              >\n                <v-list-item-icon>\n                  <v-icon v-text=\"item.icon\"></v-icon>\n                </v-list-item-icon>\n                <v-list-item-content>\n                  <v-list-item-title v-text=\"item.text\"></v-list-item-title>\n                </v-list-item-content>\n              </v-list-item>\n            </v-list-item-group>\n          </v-list>\n        </v-card>\n      </v-menu>\n    </v-app-bar>\n    <v-navigation-drawer\n      v-model=\"sidebarMenu\"\n      app\n      floating\n      :permanent=\"sidebarMenu\"\n      :mini-variant.sync=\"miniVariant\"\n      color=\"grey darken-4\"\n    >\n      <v-list dense>\n        <v-list-item>\n          <v-list-item-action>\n            <v-icon @click.stop=\"sidebarMenu = !sidebarMenu\" dark\n              >mdi-chevron-left</v-icon\n            >\n          </v-list-item-action>\n          <v-list-item-content>\n            <v-list-item-title></v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n      </v-list>\n      <v-list-item class=\"px-2\" @click=\"miniVariant = !miniVariant\" dark>\n        <v-list-item-avatar>\n          <v-icon>mdi-account-outline</v-icon>\n        </v-list-item-avatar>\n        <v-list-item-content class=\"text-truncate\">\n          Mark Anthony Vale\n        </v-list-item-content>\n        <v-btn icon small>\n          <v-icon>mdi-chevron-left</v-icon>\n        </v-btn>\n      </v-list-item>\n      <v-divider color=\"white\"></v-divider>\n      <v-list shaped>\n        <v-list-item\n          v-for=\"item in items\"\n          :key=\"item.title\"\n          link\n          :to=\"item.href\"\n          active-class=\"primary--text\"\n        >\n          <v-list-item-icon>\n            <v-icon color=\"primary\">{{ item.icon }}</v-icon>\n          </v-list-item-icon>\n          <v-list-item-content>\n            <v-list-item-title class=\"primary--text\">{{\n              item.title\n            }}</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n      </v-list>\n\n      <v-list shaped>\n        <v-list-group\n          v-for=\"item in items_2\"\n          :key=\"item.title\"\n          v-model=\"item.active\"\n         \n          no-action\n          active-class=\"primary--text\"\n        >\n          <template v-slot:activator>\n            <v-list-item-icon>\n                <v-icon color=\"primary\">{{ item.action }}</v-icon>\n            </v-list-item-icon>\n            <v-list-item-content>\n              <v-list-item-title v-text=\"item.title\" class=\"primary--text\"></v-list-item-title>\n            </v-list-item-content>\n          </template>\n\n          <v-list-item v-for=\"child in item.items\" :key=\"child.title\" :to=\"child.href\" class=\"b__left text-decoration-none\">\n            <div class=\"\">\n              <v-list-item-content>\n              <v-list-item-title v-text=\"child.title\"  class=\"primary--text text-caption\"></v-list-item-title>\n            </v-list-item-content>\n            </div>\n            \n          </v-list-item>\n        </v-list-group>\n      </v-list>\n    </v-navigation-drawer>\n    <!-- <v-content> -->\n    <v-main>\n      <v-container class=\"px-4 py-0 fill-height\" fluid>\n        <v-row class=\"fill-height\">\n          <v-col>\n            <transition name=\"fade\">\n              <router-view />\n            </transition>\n          </v-col>\n        </v-row>\n      </v-container>\n      <v-footer app color=\"grey darken-4\" class=\"py-3\" dark>\n        <span class=\"ml-auto overline\">MMI &copy;2021</span>\n      </v-footer>\n    </v-main>\n    <!-- </v-content> -->\n\n    <!-- <Navbar /> -->\n    <!-- </v-card> -->\n  </div>\n\n  <!-- </v-container> -->\n</template>\n\n<script>\n// import Navbar from \"@/components/Navbar\";\nimport { mapGetters } from \"vuex\";\nimport { axiosBase } from \"@/api/axiosConfig\";\nexport default {\n  data() {\n    return {\n      user: {},\n      drawer: true,\n      items: [\n        {\n          title: \"Dashboard\",\n          icon: \"mdi-view-dashboard\",\n          href: { name: \"dashboard\" },\n        },\n        { title: \"Reports\", icon: \"mdi-table\", href: { name: \"reports\" } },\n        // {\n        //   title: \"Website\",\n        //   icon: \"mdi-table\",\n        //   href: { name: \"reports\" },\n        //   items: [{ text: test, href: { name: \"reports\" } }],\n        // },\n        { title: \"Test View\", icon: \"mdi-help-box\", href: { name: \"test\" } },\n      ],\n      items_2: [\n        {\n          action: \"mdi-view-dashboard\",\n          items: null,\n          title: \"Dashboard\",\n          href: {name: \"dashbaord\"}\n        },\n        {\n          action: \"mdi-table\",\n          active: true,\n          items: [\n            { title: \"Dashboard Reports\" , href: { name: \"reports\" }},\n            { title: \"Error Reports\" },\n            { title: \"Download Latency Reports\" },\n          ],\n          title: \"Website\",\n          href: null\n        },\n        {\n          action: \"mdi-silverware-fork-knife\",\n          active: true,\n          items: [\n            { title: \"Breakfast & brunch\" },\n            { title: \"New American\" },\n            { title: \"Sushi\" },\n          ],\n          title: \"Dining\",\n          href: {name: \"dashbaord\"}\n        },\n      ],\n      color: \"primary\",\n      colors: [\"primary\", \"blue\", \"success\", \"red\", \"teal\"],\n      right: false,\n      sidebarMenu: true,\n      miniVariant: false,\n      expandOnHover: false,\n      background: false,\n      item: 1,\n      userDetails: [\n        { text: \"Profile\", icon: \"mdi-account\" },\n        { text: \"Change Password\", icon: \"mdi-lock\" },\n        { text: \"Logout\", icon: \"mdi-logout\", href: { name: \"logout\" } },\n      ],\n    };\n  },\n  components: {\n    // Navbar,\n  },\n  computed: {\n    ...mapGetters([\"isAuthenticated\"]),\n    currentUser() {\n      if (this.isAuthenticated) {\n        const crrUser = this.$store.state.user.user;\n        return crrUser;\n      } else {\n        const temp_user = {\n          username: \"None\",\n        };\n        return temp_user;\n      }\n    },\n    bg() {\n      return this.background\n        ? \"https://cdn.vuetifyjs.com/images/backgrounds/bg-2.jpg\"\n        : undefined;\n    },\n  },\n  mounted() {\n    this.getCurrentUser();\n    console.log(\"Navbar Component Mounted\");\n  },\n  methods: {\n    getCurrentUser() {\n      console.log(this.$store.state.user.user);\n      if (this.isAuthenticated) {\n        axiosBase\n          .get(\"auth/user/\", {\n            headers: {\n              \"Content-Type\": \"application/x-www-form-urlencoded\",\n              Authorization: `Token ${localStorage.getItem(\"token\")}`,\n            },\n          })\n          .then((res) => {\n            // this.user = res.data;\n            this.$store.commit(\"currentUser\", res.data);\n            this.user = this.$store.state.user.user;\n          })\n          .catch((err) => console.log(err));\n      }\n    },\n  },\n};\n</script>\n<style>\n.b__left{\n  border-left: 5px solid red !important;\n  position: absolute;\n  right: -30px;\n}\n</style>"]}]}